{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "import/extensions":"off",
        "no-plusplus":"off",
        "no-underscore-dangle":"off",
        "import/prefer-default-export":"off",
        "prefer-promise-reject-errors":"off"
    }
}
